<?xml version="1.0" encoding="utf-8"?>
<Launcher
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="projekt-XSD.xsd">
  <Mainform>
    <width>100%</width>
    <height>100%</height>
    <adjustable>No</adjustable>
    <controls>No</controls>
    <movable>No</movable>
    <defaultPosition>Center</defaultPosition>
    <opacity>100%</opacity>
    <text>Launcher</text>
    <server_path>/data/versions/</server_path>
    <local_path>/data/versions/</local_path>
    <data_check>No</data_check>
    <update_required>No</update_required>
    <login_button>
        <visible>Yes</visible>
        <width>60px</width>
        <height>30px</height>
    </login_button>
    <background_image>
      <path>/data/pictures/background/</path>
      <visible>Yes</visible>
      <width>100%</width>
      <heigth>100%</heigth>
    </background_image>
    <label1>
      <visible>No</visible>
      <width>120px</width>
      <heigth>60px</heigth>
    </label1>
    <gallery>
      <visible>Yes</visible>
      <width>120px</width>
      <heigth>60px</heigth>
      <image_source>
        <path>/data/pictures/gallery/</path>
        <ending>.jpg</ending>
        <max_count>5</max_count>
      </image_source>
      <slideshow>Yes</slideshow>
      <slide_time>15</slide_time>
    </gallery>
    <progressBar>
      <value>0</value>
      <visible>No</visible>
      <width>120px</width>
      <heigth>60px</heigth>
      <increaseBy>2</increaseBy>
    </progressBar>
  </Mainform>
  
  <InputForm>
    <width>100%</width>
    <height>100%</height>
    <adjustable>Yes</adjustable>
    <controls>No</controls>
    <movable>Yes</movable>
    <defaultPosition>Center</defaultPosition>
    <opacity>100%</opacity>
    <text>Feedback</text>
    <heading_textbox>      
      <width>120px</width>
      <heigth>60px</heigth>
      <max_lenght>50</max_lenght>
      <text>Enter name</text>
    </heading_textbox>
    <body_textbox>      
      <width>120px</width>
      <heigth>60px</heigth>
      <max_lenght>550</max_lenght>
      <text>Enter description</text>
    </body_textbox>
    <heading_label>      
      <width>120px</width>
      <heigth>60px</heigth>
      <text>Name</text>      
    </heading_label>
    <body_label>
      <width>120px</width>
      <heigth>60px</heigth>
      <text>Description</text>      
    </body_label>
    <send_button>      
      <width>120px</width>
      <heigth>60px</heigth>
      <text>Send</text>
    </send_button>
    <close_button>      
      <width>120px</width>
      <heigth>60px</heigth>
      <text>Close</text>
    </close_button>
  </InputForm>

  <Loginform>
    <width>100%</width>
    <height>100%</height>
    <adjustable>No</adjustable>
    <controls>No</controls>
    <movable>No</movable>
    <defaultPosition>Center</defaultPosition>
    <text>Login</text>
    <login_label>
      <width>120px</width>
      <heigth>60px</heigth>
      <text>Nickname: </text>
    </login_label>
    <pssd_label>
      <width>120px</width>
      <heigth>60px</heigth>
      <text>Password: </text>
    </pssd_label>
    <login_textBox>
      <width>120px</width>
      <heigth>60px</heigth>
      <text>Enter Nickname Here</text>
      <max_input>20</max_input>
    </login_textBox>
    <pssd_textBox>
      <width>120px</width>
      <heigth>60px</heigth>
      <text>Enter Password Here</text>
      <char>*</char>
      <max_input>20</max_input>
    </pssd_textBox>
    <image>
      <width>120px</width>
      <heigth>60px</heigth>
    </image>
    <login_button>
      <width>120px</width>
      <heigth>60px</heigth>
      <text>Login</text>
    </login_button>
    <cancel_button>
      <width>120px</width>
      <heigth>60px</heigth>
      <text>Cancel</text>
    </cancel_button>
  </Loginform>
</Launcher>
