<?xml version="1.0" encoding="utf-8"?>
<Launcher
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="projekt-XSD.xsd">
  <Mainform>
    <width>500px</width>
    <height>500px</height>
    <adjustable>No</adjustable>
    <controls>No</controls>
    <movable>No</movable>
    <defaultPosition>Center</defaultPosition>
    <opacity>100%</opacity>
    <text>Launcher</text>
    <button1>
        <visible>No</visible>
        <width>60px</width>
        <heigth>30px</heigth>
    </button1>
    <image>
      <visible>Yes</visible>
      <width>500px</width>
      <heigth>500px</heigth>
    </image>
    <label1>
      <visible>No</visible>
      <width>120px</width>
      <heigth>60px</heigth>
    </label1>
    <gridView>
      <visible>No</visible>
      <width>120px</width>
      <heigth>60px</heigth>
    </gridView>
    <progressBar>
      <value>0</value>
      <visible>No</visible>
      <width>120px</width>
      <heigth>60px</heigth>
      <increaseBy>2</increaseBy>
    </progressBar>
  </Mainform>
  
  <InputForm>
    <width>300px</width>
    <height>240px</height>
    <adjustable>Yes</adjustable>
    <controls>No</controls>
    <movable>Yes</movable>
    <defaultPosition>Center</defaultPosition>
    <opacity>100%</opacity>
    <text>InputForm</text>
    <textbox1>      
      <width>120px</width>
      <heigth>60px</heigth>
    </textbox1>
    <textbox2>      
      <width>120px</width>
      <heigth>60px</heigth>
    </textbox2>
    <label1>      
      <width>120px</width>
      <heigth>60px</heigth>
    </label1>
    <label2>
      <width>120px</width>
      <heigth>60px</heigth>
    </label2>
    <button1>      
      <width>120px</width>
      <heigth>60px</heigth>
    </button1>
  </InputForm>

  <Loginform>
    <width>300px</width>
    <height>240px</height>
    <adjustable>Yes</adjustable>
    <controls>No</controls>
    <movable>Yes</movable>
    <defaultPosition>Center</defaultPosition>
    <opacity>100%</opacity>
    <text>LoginForm</text>
    <label1>
      <width>120px</width>
      <heigth>60px</heigth>
    </label1>
    <label2>
      <width>120px</width>
      <heigth>60px</heigth>
    </label2>
    <textBox1>
      <width>120px</width>
      <heigth>60px</heigth>
    </textBox1>
    <textBox2>
      <width>120px</width>
      <heigth>60px</heigth>
    </textBox2>
    <image>
      <width>120px</width>
      <heigth>60px</heigth>
    </image>
    <button1>
      <width>120px</width>
      <heigth>60px</heigth>
    </button1>
    <button2>
      <width>120px</width>
      <heigth>60px</heigth>
    </button2>
  </Loginform>
</Launcher>

